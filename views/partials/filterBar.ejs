<div class="filter-container">

<p>One hour interval only</p>
    <form action="/search/submit" method="POST" id="filterForm">
        <label for="venue">Choose a venue:</label>
            <select id="venue" name="venue">
                <option value="KTDI">KTDI</option>
                <option value="saab">KTHO</option>
                <option value="fiat">KTC</option>
                <option value="audi">K9/K10</option>
            </select>
        <label for="date">Date:</label>
        <input type="date" name="date" required id="datePickerId"/>
        <label for="time-start">Start time</label>
        <!-- <input id="time" type="time"> -->
        <select id="time" name="timestart" required >
            <option value="12">12:00pm</option>
            <option value="13">1:00pm</option>
            <option value="14">2:00pm</option>
            <option value="15">3:00pm</option>
            <option value="16">4:00pm</option>
            <option value="17">5:00pm</option>
            <option value="18">6:00pm</option>
            <option value="19">7:00pm</option>
            <option value="20">8:00pm</option>
            <option value="21">9:00pm</option>
            <option value="22">10:00pm</option>
            
        </select>
        <select name="duration" id="duration" required>
            <option value="1 hour">1 hour</option>
        </select>
        
        <input type="submit" value="Find!">
    </form>
</div>
<script>
    datePickerId.min  = new Date().toLocaleDateString('en-ca')
</script>